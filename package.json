{
  "name": "redux-universal",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run build",
    "dev-universal": "better-npm-run dev-universal",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "dev-universal": {
      "command": "webpack --config webpack/webpack.config.js && node ./bin/server.universal",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node ./bin/server.universal",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node ./bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "webpack --config webpack/webpack.config.js && mocha --require babel-register './tests/**/*.test.js' -t 25000"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joehua87/redux-koa-universal.git"
  },
  "keywords": [
    "redux",
    "koa",
    "universal",
    "react",
    "isomorphic"
  ],
